digraph mapping { 
	margin=0.05;
	rankdir=LR;
	
	// ------------------------------------------------------
	// exemple de mapping
	// ------------------------------------------------------
	node [shape=record];
	elt  [label="<f0> guidoelement | <f1> name | <f2> attributes"];
	attr [label="<f0> guidoattribute | <f1> name | <f2> value | <f3> unit (opt.)"];

	elt:f2 -> attr:f0 [arrowtail=dot, label="0...n"];
}
